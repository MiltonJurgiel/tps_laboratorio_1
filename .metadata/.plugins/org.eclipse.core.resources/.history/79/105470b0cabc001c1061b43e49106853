#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	setbuf(stdout, NULL);
	int opcion;


	do
	{
		printf("1. Ingresar kilometros: \n"
				"2. Ingresar precio de Vuelos: (Aerolineas=y, Latam=z): \n"
				"3. Calcular\n"
				"4. Informar resultados\n"
				"5. Carga forzada de datos\n"
				"6. Salir\n\n");
		opcion=ingresarOpcion();


		switch(opcion)
		{
		case 1:
			printf("estoy en la funcion 1");



		break;
		case 2:




			printf("Estoy en el 2");
		break;
		case 3:



		break;
		case 4:




		break;
		default:


		break;


		}

	}while(opcion!=6);


	return EXIT_SUCCESS;
}
